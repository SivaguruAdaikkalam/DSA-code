#include <stdio.h>
#include <string.h>
#include <stdlib.h>

#define MAX 10
#define LEN 50

int main() {
    int dollars, items;
    int purchased_count = 0;
    int i;

    char name[MAX][LEN];
    int price[MAX];
    int afford[MAX];

    scanf("%d %d", &dollars, &items);

    for(i=0;i<items;i++) {
        scanf("%s %d", name[i], &price[i]);
        afford[i] = 0;
    }

    for(i=0;i<items;i++) {
        if (dollars >= price[i]) {
            dollars -= price[i];
            purchased_count++;
            afford[i] = 1;  }
    }
    for(i=0;i<items;i++) {
        if (afford[i])
            printf("I can afford %s\n", name[i]);
        else
            printf("I can't afford %s\n", name[i]);
    }
    if (purchased_count == 0)
        printf("I need more Dollar!\n");
    else
        printf("%d\n", dollars);
    return 0;
}
